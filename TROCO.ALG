Algoritmo "TROCO"

Var
QUANT:INTEIRO
VALOR_RE,PRECO,TROCO,VALOR_TOTAL :REAL


Inicio
      ESCREVA("DIGITE O VALOR DO PRODUTO ")
      LEIA(PRECO)
      ESCREVA("DIGITE A QUANTIDADE DO PRODUTO ")
      LEIA(QUANT)
      VALOR_TOTAL <- PRECO*QUANT
      ESCREVA("DIGITE O VALOR RECEBIDO ")
      LEIA(VALOR_RE)
      TROCO <- VALOR_RE-VALOR_TOTAL
      
      ESCREVAL("SEU TROCO FICA ",TROCO:4:2)
      




Fimalgoritmo